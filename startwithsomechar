
public class StartWtihSomeChar implements CheckStrategy{
	   private char c;
	   
       public StartWtihSomeChar(char c){
       this.c = c;
       }
       
       
       public boolean check(String s) {
   		if ( s == null || s.length() == 0) {
   			return false;
   		}
   		return s.charAt(0) == c;
   	}
       
}
